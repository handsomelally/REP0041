ALTER   PROCEDURE [dbo].[sp_REP0041_Param_SN_Brand]
AS
BEGIN
    SET NOCOUNT ON;

    SELECT DISTINCT
        Brand
    FROM [PPCGIVDWHSQL01\EXTERNALDATA,1434].ServiceNowControl.Reference.ServiceNowProducts
    WHERE Brand IS NOT NULL
    ORDER BY Brand;
END
