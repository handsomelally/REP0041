ALTER   PROCEDURE [dbo].[sp_REP0041_Param_SourceSystem]
AS
BEGIN
    SET NOCOUNT ON;

    SELECT DISTINCT
        SourceSystem
    FROM [PPCGIVDWHSQL01\EXTERNALDATA,1434].ServiceNowControl.Reference.Products
    WHERE SourceSystem IS NOT NULL
    ORDER BY SourceSystem;
END
