ALTER   PROCEDURE [dbo].[sp_REP0041_Param_ProductType]
AS
BEGIN
    SET NOCOUNT ON;

    SELECT DISTINCT
        ProductType
    FROM [PPCGIVDWHSQL01\EXTERNALDATA,1434].ServiceNowControl.Reference.Products
    WHERE ProductType IS NOT NULL
    ORDER BY ProductType;
END
