ALTER   PROCEDURE [dbo].[sp_REP0041_Param_Bundles]
AS
BEGIN
    SET NOCOUNT ON;
	A
    SELECT DISTINCT
        BundleID,
        DisplayName
    FROM [PPCGIVDWHSQL01\EXTERNALDATA,1434].ServiceNowControl.Reference.Bundles
    WHERE BundleID IS NOT NULL
    ORDER BY DisplayName;
END
